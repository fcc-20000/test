<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.test.dao.OutwarehouseDao">

    <resultMap type="com.demo.test.entity.Outwarehouse" id="OutwarehouseMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="wid" column="wid" jdbcType="INTEGER"/>
        <result property="aid" column="aid" jdbcType="INTEGER"/>
        <result property="sid" column="sid" jdbcType="INTEGER"/>
        <result property="founder" column="founder" jdbcType="VARCHAR"/>
        <result property="reviewer" column="reviewer" jdbcType="VARCHAR"/>
        <result property="reviewtime" column="reviewTime" jdbcType="TIMESTAMP"/>
        <result property="creationtime" column="creationTime" jdbcType="TIMESTAMP"/>
        <result property="remarks" column="remarks" jdbcType="VARCHAR"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="OutwarehouseMap">
        select
          id, wid, aid, sid, founder, reviewer, reviewTime, creationTime, remarks
        from warehouse2db.outwarehouse
        where id = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="OutwarehouseMap">
        select
          id, wid, aid, sid, founder, reviewer, reviewTime, creationTime, remarks
        from warehouse2db.outwarehouse
        limit #{offset}, #{limit}
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="OutwarehouseMap">
        select
          id, wid, aid, sid, founder, reviewer, reviewTime, creationTime, remarks
        from warehouse2db.outwarehouse
        <where>
            <if test="id != null">
                and id = #{id}
            </if>
            <if test="wid != null">
                and wid = #{wid}
            </if>
            <if test="aid != null">
                and aid = #{aid}
            </if>
            <if test="sid != null">
                and sid = #{sid}
            </if>
            <if test="founder != null and founder != ''">
                and founder = #{founder}
            </if>
            <if test="reviewer != null and reviewer != ''">
                and reviewer = #{reviewer}
            </if>
            <if test="reviewtime != null">
                and reviewTime = #{reviewtime}
            </if>
            <if test="creationtime != null">
                and creationTime = #{creationtime}
            </if>
            <if test="remarks != null and remarks != ''">
                and remarks = #{remarks}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into warehouse2db.outwarehouse(wid, aid, sid, founder, reviewer, reviewTime, creationTime, remarks)
        values (#{wid}, #{aid}, #{sid}, #{founder}, #{reviewer}, #{reviewtime}, #{creationtime}, #{remarks})
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update warehouse2db.outwarehouse
        <set>
            <if test="wid != null">
                wid = #{wid},
            </if>
            <if test="aid != null">
                aid = #{aid},
            </if>
            <if test="sid != null">
                sid = #{sid},
            </if>
            <if test="founder != null and founder != ''">
                founder = #{founder},
            </if>
            <if test="reviewer != null and reviewer != ''">
                reviewer = #{reviewer},
            </if>
            <if test="reviewtime != null">
                reviewTime = #{reviewtime},
            </if>
            <if test="creationtime != null">
                creationTime = #{creationtime},
            </if>
            <if test="remarks != null and remarks != ''">
                remarks = #{remarks},
            </if>
        </set>
        where id = #{id}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from warehouse2db.outwarehouse where id = #{id}
    </delete>

</mapper>